    "diffEditor.removedTextBackground": "#88063852"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.quoted.docstring.multi"
      ],
      "settings": {
        "foreground": "#909090",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#6080ff"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff0e6b"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#40c4aa"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#48fb9f"
      }
    },
    {
      "scope": [
        "string",
        "markup.inline"
      ],
      "settings": {
        "foreground": "#8bbb81"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "number"
      ],
      "settings": {
        "foreground": "#c080ff"
      }
    },
    {
      "scope": [
        "storage.type",
        "entity.name.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#ff9020"
      }
    },
    {
      "scope": [
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#a0ff20"
      }
    },
    {
      "scope": "markup.error",
      "settings": {
        "foreground": "#e54c4c"
      }
    },
    {
      "scope": "markup.warning",
      "settings": {
        "foreground": "#f2be59"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#54aa46"
      }
    }
  ]
}
